{
  "name": "template-nuxt-supabase",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "export:schema": "node scripts/export_db_schema.mjs --database qualitec_catalogos",
    "apply:schema:produtos": "node scripts/apply_db_schema.mjs --file db/qualitec_catalogos/produtos.sql",
    "apply:schema:produtos:reorder": "node scripts/run_sql.mjs --file db/qualitec_catalogos/produtos.reorder.sql",
    "import:manometros": "python scripts/import_produtos.py --file \"tabela manometros.csv\" --delimiter \";\" --encoding \"utf-8-sig\"",
    "import:supabase:csv": "node scripts/import_supabase_csv.mjs \"tabela manometros.csv\""
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.72",
    "@iconify-json/uil": "^1.2.3",
    "@nuxt/ui": "^4.1.0",
    "@nuxtjs/supabase": "2.0.1",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@supabase/supabase-js": "^2.81.1",
    "@vueuse/core": "^14.0.0",
    "csv-parse": "^6.1.0",
    "flowbite": "^3.1.2",
    "jszip": "^3.10.1",
    "mysql2": "^3.15.3",
    "nodemailer": "^7.0.10",
    "nuxt": "4.1.3",
    "pdfkit": "^0.17.2",
    "vue": "3.5.22",
    "vue-router": "4.6.3",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "@unocss/nuxt": "^66.5.5",
    "ngrok": "^5.0.0-beta.2",
    "unocss": "^66.5.5"
  }
}
