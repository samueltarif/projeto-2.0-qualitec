{"file":"teste-filtros-C3XAXZOE.js","mappings":";;;;;;;;;;;;;;;;;;AA6BA,UAAM,UAAU,IAAI,IAAI;AACxB,UAAM,QAAQ,IAAI,EAAE;AACpB,UAAM,UAAU,IAAI,CAAA,CAAE;AACtB,UAAM,UAAU,IAAI,IAAI;AAExB,UAAM,YAAY,SAAS,OAAO;AAAA,MAChC,SAAS,QAAQ;AAAA,MACjB,SAAS,QAAQ;AAAA,MACjB,SAAS,QAAQ;AAAA,MACjB,OAAO,MAAM;AAAA,IACf,EAAE;;;UApCa,QAAA,OAAO;;iBACF,MAAA,OAAK;yEAAyB,MAAA,KAAK,CAAA,QAAA;AAAA;;AAE3BA,sBAAA,QAAA,QAAV,WAAM;AACT,gBAAA,mEAAAC,eAAA,OAAO,KAAK,CAAA,OAAA;AACL,cAAA,OAAO,WAAW,OAAO,QAAQ,QAAM;;0BAE1B,OAAO,SAAO,CAAxB,WAAM;8BAAmCC,cAAA,SAAO,MAAM,oCAChE,MAAM;;;;;;;;;;0HAUP,UAAA,KAAS,CAAA,oBAAA;AAAA;;;;;;;;;;;;;;;ACpBhB,YAAA,OAAAC,eAAAC,WAAA,EAAA,OAAM,SAAK,MAAA,CAAA,CAAA,4DAAA;;;;;;;;;;;;","names":["_ssrRenderList","_ssrInterpolate","_ssrRenderAttr","_ssrRenderAttrs","_mergeProps"],"sources":["../../../../../../../app/components/TesteFiltros.vue","../../../../../../../app/pages/teste-filtros.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Teste de Filtros</h2>\n    <div v-if=\"loading\">Carregando filtros...</div>\n    <div v-else-if=\"error\" class=\"error\">Erro: {{ error }}</div>\n    <div v-else>\n      <div v-for=\"filter in filters\" :key=\"filter.key\" class=\"filter-section\">\n        <h3>{{ filter.label }}</h3>\n        <select v-if=\"filter.options && filter.options.length\">\n          <option value=\"\">Todos</option>\n          <option v-for=\"option in filter.options\" :key=\"option\" :value=\"option\">\n            {{ option }}\n          </option>\n        </select>\n        <p v-else class=\"empty\">Sem opções disponíveis</p>\n      </div>\n    </div>\n    \n    <!-- Debug info -->\n    <div class=\"debug\">\n      <h3>Debug Info:</h3>\n      <pre>{{ debugInfo }}</pre>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, computed } from 'vue'\n\nconst loading = ref(true)\nconst error = ref('')\nconst filters = ref([])\nconst apiData = ref(null)\n\nconst debugInfo = computed(() => ({\n  filters: filters.value,\n  apiData: apiData.value,\n  loading: loading.value,\n  error: error.value\n}))\n\nonMounted(async () => {\n  try {\n    console.log('Iniciando carregamento de filtros...')\n    \n    const response = await fetch('/api/produtos/distinct?categoria=Manômetros&limit=500')\n    const data = await response.json()\n    \n    console.log('Dados recebidos:', data)\n    apiData.value = data\n    \n    if (data.values) {\n      console.log('data.values.tipo_medicao:', data.values.tipo_medicao)\n      console.log('data.values.certificados:', data.values.certificados)\n      \n      // Mapear os filtros que queremos testar\n      filters.value = [\n        {\n          key: 'tipo_medicao',\n          label: 'Tipo de Medição',\n          options: data.values.tipo_medicao || []\n        },\n        {\n          key: 'certificados', \n          label: 'Certificados',\n          options: data.values.certificados || []\n        }\n      ]\n      \n      console.log('Filtros processados:', filters.value)\n    }\n    \n    loading.value = false\n  } catch (err) {\n    console.error('Erro ao carregar filtros:', err)\n    error.value = err.message\n    loading.value = false\n  }\n})\n</script>\n\n<style scoped>\n.filter-section {\n  margin: 15px 0;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n}\n\n.error {\n  color: red;\n  background: #ffe6e6;\n  padding: 10px;\n  border-radius: 5px;\n}\n\n.empty {\n  color: #666;\n  font-style: italic;\n}\n\n.debug {\n  margin-top: 20px;\n  padding: 10px;\n  background: #f0f0f0;\n  border-radius: 5px;\n  font-size: 12px;\n}\n\n.debug pre {\n  white-space: pre-wrap;\n}\n</style>","<template>\n  <div class=\"p-8\">\n    <h1 class=\"text-2xl font-bold mb-6\">Teste de Filtros</h1>\n    <TesteFiltros />\n  </div>\n</template>\n\n<script setup>\nimport TesteFiltros from '~/components/TesteFiltros.vue'\n</script>"],"version":3}